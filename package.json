{
  "name": "notes-to-website-tools",
  "version": "1.1.0",
  "description": "Small tooling to generate TOC JSON from content files",
  "main": "scripts/generate_toc.js",
  "scripts": {
    "gen-toc": "node scripts/generate_toc.js",
    "process-boxes": "node scripts/process_boxes.js",
    "cap:init": "npx @capacitor/cli init --web-dir mobile",
    "cap:add-android": "npx cap add android",
    "cap:sync": "npx cap sync",
    "cap:copy": "npx cap copy",
    "cap:open-android": "npx cap open android",
    "cap:build": "npx cap sync && npx cap open android"
  },
  "dependencies": {
    "@capacitor/app": "^5.0.0",
    "axios": "^1.13.2",
    "body-parser": "^2.2.1",
    "cheerio": "^1.1.2",
    "express": "^5.2.1",
    "mammoth": "^1.4.18",
    "marked": "^5.0.0"
  },
  "devDependencies": {
    "@capacitor/android": "^5.0.0",
    "@capacitor/cli": "^5.0.0",
    "@capacitor/core": "^5.0.0"
  }
}
